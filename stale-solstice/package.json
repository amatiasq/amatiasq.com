{
  "name": "stale-solstice",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bin/generate-i18n-files.sh && astro dev",
    "start": "npm run dev",
    "build": "bin/generate-i18n-files.sh --copy && astro build",
    "preview": "bin/generate-i18n-files.sh && astro preview",
    "astro": "astro",
    "i18n": "bin/generate-i18n-files.sh",
    "lint": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@astrojs/solid-js": "^3.0.1",
    "astro": "^3.0.10",
    "solid-js": "^1.7.11"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.3",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "sass": "^1.66.1"
  }
}
