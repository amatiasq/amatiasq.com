{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@util/*": [
        "src/util/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}